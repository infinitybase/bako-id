{
  "rewrites": [
    {
      "source": "/marketplace/(.*)",
      "destination": "/garage.html"
    },
    {
      "source": "/marketplace",
      "destination": "/garage.html"
    },
    {
      "source": "/(.*)",
      "destination": "/garage.html",
      "has": [
        {
          "type": "host",
          "value": "garage.zone"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/garage.html",
      "has": [
        {
          "type": "host",
          "value": "(.+\\.)?garage\\.zone"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/bako.html"
    }
  ]
}